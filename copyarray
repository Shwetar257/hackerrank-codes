#include <stdio.h>
int main(int argc,char const *argv[])
{
   int ar[5],ar1[5];
   int i,j;
   for(int i=0;i<5;i++)
   {
       printf("enter the elements in first array=> ");
       scanf("%d",&ar[i]);
   }
    for(i=0;i<5;i++)
    {
      ar1[i]=ar[i];
    }
    printf("elements stored in first array are=> ");
     for(i=0;i<5;i++)
     {
         printf("%d",ar[i]);
     }
     printf(" elements in second array=> ");
      for(i=0;i<5;i++)
      {
          printf("%d",ar1[i]);
      }
