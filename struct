#include<stdio.h>
struct  student
{
  char *name;
  int age;
} s1,s2,allstd[2];
int main()
{
    s1.name="shweta";
    s1.age=23;
    s2.name="sneha";
    s2.age=22;
    allstd[0].name="niharika";
    allstd[0].age=22;
    allstd[1].name="riya";
    allstd[1].age=22;
    printf("\nname=%s",s1.name);
    printf("\nage=%d",s1.age);
    printf("\nname=%s",s2.name);
    printf("\nage=%d",s2.age);
    printf("\nallstd[0]=%s",allstd[0].name);
    return 0;

}
